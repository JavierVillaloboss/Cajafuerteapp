<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_0da20290ab6baa9b1c010ec35565f12"
    tools:context=".Cambiouser">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fondo_amarillo">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="91dp"
            android:text="@string/Cuser"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/black"
            app:layout_constraintBottom_toTopOf="@+id/contraseniaAc"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/cambioCU"
            android:layout_width="253dp"
            android:layout_height="43dp"
            android:layout_marginTop="116dp"
            android:ems="10"
            android:hint="@string/passN"
            android:inputType="textPassword"
            android:textColor="@color/black"
            android:textColorHint="@color/gris"
            app:layout_constraintStart_toStartOf="@+id/contraseniaAc"
            app:layout_constraintTop_toBottomOf="@+id/contraseniaAc" />

        <EditText
            android:id="@+id/cambioCUN"
            android:layout_width="253dp"
            android:layout_height="40dp"
            android:layout_marginTop="116dp"
            android:ems="10"
            android:hint="@string/passNC"
            android:inputType="textPassword"
            android:textColor="@color/black"
            android:textColorHint="@color/gris"
            app:layout_constraintStart_toStartOf="@+id/cambioCU"
            app:layout_constraintTop_toBottomOf="@+id/cambioCU" />

        <EditText
            android:id="@+id/contraseniaAc"
            android:layout_width="250dp"
            android:layout_height="0dp"
            android:layout_marginBottom="567dp"
            android:ems="10"
            android:hint="@string/passAc"
            android:inputType="textPassword"
            android:textColor="@color/black"
            android:textColorHint="@color/gris"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <Button
            android:id="@+id/cambioCUbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="128dp"
            android:text="@string/aceptar"
            android:textColorLink="@color/yellow_300"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cambioCUN" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>